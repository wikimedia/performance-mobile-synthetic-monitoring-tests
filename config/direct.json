{
    "extends": "../../secrets.json",
    "utc": true,
    "gzipHAR": true,
    "html": {
      "fetchHARFiles": true,
      "compareURL": "https://compare.sitespeed.io/"
    },
    "screenshot": {
      "type": "jpg"
    },
    "plugins": {
      "remove": [
        "thirdparty",
        "coach"
      ]
    },
    "graphite": {
      "messages": ["pageSummary", "run"]
    },
    "copyLatestFilesToBaseGraphiteNamespace": true,
    "android": true,
    "browsertime": {
      "cpu": true,
      "iterations": 7,
      "limitedRunData": true,
      "preWarmServer": true,
      "preWarmServerWaitTime": 10000,
      "timeToSettle": 20000,
      "visualElements": true,
      "cacheClearRaw": true,
      "video": true,
      "visualMetrics": true,
      "visualMetricsPortable": true,
      "androidRooted": true,
      "androidVerifyNetwork": true,
      "chrome": {
        "traceCategory": ["disabled-by-default-v8.cpu_profiler"],
        "collectConsoleLog": true,
        "includeResponseBodies": "all",
        "enableTraceScreenshots": true
      }
    }
  }